           <!-- Modal -->
<div class="modal fade" id="myModalMaterial" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Cerrar</span></button>
        <h4 class="modal-title" id="myModalLabel">Nombre del material</h4>
      </div>
      <div class="modal-body">
            <% Material.all.all.each do |m|  %>
        <li><%= check_box_tag "material_ids[]", m.id %> <%= m.nombre_codigo_material %></li>
            <% end %>
        
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Volver</button>
      </div>
    </div>
  </div>
</div>