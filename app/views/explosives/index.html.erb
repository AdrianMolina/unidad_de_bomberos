<h1>Listing explosives</h1>

<table>
  <thead>
    <tr>
      <th>Numero caso</th>
      <th>Fecha</th>
      <th>Lugar incidente</th>
      <th>Hora incidente</th>
      <th>Descripcion incidente</th>
      <th>Material utilizado</th>
      <th>Observaciones incidente</th>
      <th>Jefe brigada</th>
      <th>Jefe seguridad</th>
      <th>Emergency</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @explosives.each do |explosive| %>
      <tr>
        <td><%= explosive.numero_caso %></td>
        <td><%= explosive.fecha %></td>
        <td><%= explosive.lugar_incidente %></td>
        <td><%= explosive.hora_incidente %></td>
        <td><%= explosive.descripcion_incidente %></td>
        <td><%= explosive.material_utilizado %></td>
        <td><%= explosive.observaciones_incidente %></td>
        <td><%= explosive.jefe_brigada %></td>
        <td><%= explosive.jefe_seguridad %></td>
        <td><%= explosive.emergency.id %></td>
        <td><%= link_to 'Show', explosive %></td>
        <td><%= link_to 'Edit', edit_explosife_path(explosive) %></td>
        <td><%= link_to 'Destroy', explosive, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Explosive', new_explosife_path %>
